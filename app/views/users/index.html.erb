

<% @users.each do |user| %>

<div class="user">
	<p><%= user.first_name %> <%= user.last_name %> <%= user.email %></p>
	
	<%= link_to "Users Show #{user.full_name}", user, remote: true %>
	<%= link_to "Edit", edit_user_path(user), remote: true %>
	<%= link_to "Destroy", user, method: :delete, remote: true, class: 'destroyer' %>
</div>

<% end %>

